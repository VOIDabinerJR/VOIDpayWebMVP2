        <script>
            // Modal 1
            var modal1 = document.getElementById("createAppModal");
            var btn1 = document.getElementById("createAppBtn");
            var span1 = document.getElementById("closeModalBtn");

            if (btn1) {
                btn1.onclick = function () {
                    modal1.style.display = "block";
                }
            }

            span1.onclick = function () {
                modal1.style.display = "none";
            }

            window.onclick = function (event) {
                if (event.target == modal1) {
                    modal1.style.display = "none";
                }
            }

            // Modal 2
            var modal2 = document.getElementById("createAppModal2");
            var btn2 = document.getElementById("createAppBtn2");
            var span2 = document.getElementById("closeModalBtn2");

            if (btn2) {
                btn2.onclick = function () {
                    modal2.style.display = "block";
                }
            }

            span2.onclick = function () {
                modal2.style.display = "none";
            }

            window.onclick = function (event) {
                if (event.target == modal2) {
                    modal2.style.display = "none";
                }
            }
        </script>


