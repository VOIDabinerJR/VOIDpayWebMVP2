        <!-- Modal 2 -->
        <div id="createAppModal2" class="modal">
            <div class="modal-content">
                <span class="close" id="closeModalBtn2">&times;</span>
                <form id="form1" method="POST" action="integracao/activate">
                    <h2>Ativar</h2>
