                                    <script>
                                        function adicionarValores() {
                                            const clientID = document.getElementById("clientID").value;
                                            const buttonToken = document.getElementById("buttonToken").value;

                                            let codigo = document.getElementById("codigo").innerHTML;

                                            // Substitui os valores de clientID e buttonToken no código
                                            codigo = codigo.replace(/SEU_CLIENT_ID_AQUI/g, clientID);
                                            codigo = codigo.replace(/SEU_BUTTONTOKEN_AQUI/g, buttonToken);

                                            document.getElementById("codigo").innerHTML = codigo;

                                            // Salva no localStorage para persistir temporariamente
                                            localStorage.setItem("clientID", clientID);
                                            localStorage.setItem("buttonToken", buttonToken);

                                            alert("Valores adicionados com sucesso!");
                                        }

                                        function copiarCodigo() {
                                            const codigo = document.getElementById("codigo").innerText;
                                            navigator.clipboard.writeText(codigo).then(function () {
                                                alert("Código copiado com sucesso!");
                                            }, function () {
                                                alert("Erro ao copiar o código.");
                                            });
                                        }

                                        // Carrega os valores armazenados anteriormente, se houver
                                        window.onload = function () {
                                            if (localStorage.getItem("clientID") && localStorage.getItem("buttonToken")) {
                                                document.getElementById("clientID").value = localStorage.getItem("clientID");
                                                document.getElementById("buttonToken").value = localStorage.getItem("buttonToken");
                                            }
                                        }
                                    </script>
